## dom0 should not tell us its time.
## But if it does, create a file, so systemcheck could warn in case this
## file exists.
read timestamp
mkdir -p "/run/qubes-whonix"
echo "$timestamp" > "/run/qubes-whonix/qubes.SetDateTime"
